{
  "verbose": true,
  "moduleNameMapper": {
    "^.+\\.(css|less|scss)$": "babel-jest"
  },
  "coverageReporters": [
    "text-summary"
  ],
  "testRegex": "/*.test.js$",
  "collectCoverage": true,
  "coverageDirectory": "coverage",
  "coverageThreshold": {
    "global": {
      "branches": 0,
      "functions": 0,
      "lines": 0,
      "statements": 0
    }
  },
  "moduleDirectories": ["node_modules", "src"]

}